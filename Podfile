# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'MVVM' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Alamofire
  pod 'Alamofire', '~> 4.4'

  # RxSwift
  pod 'RxSwift',    '~> 3.0'
  pod 'RxCocoa',    '~> 3.0'

  # Realm
  pod 'RealmSwift'

  target 'MVVMTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'MVVMUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
